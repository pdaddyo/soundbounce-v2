		"react-progress-bar-plus": "^1.0.0",
		"react-redux": "^4.0.0",
		"react-router": "^2.0.0",
		"react-router-redux": "^4.0.0-beta",
		"react-select": "^1.0.0-rc.2",
		"redux": "^3.0.0",
		"redux-logger": "^2.5.2",
		"redux-thunk": "^1.0.0",
		"request": "^2.81.0",
