{
	"name": "soundbounce-v2",
	"version": "0.1.0",
	"description": "Soundbounce - the global jukebox",
	"main": "index.js",
	"engines": {
		"node": ">=4.2.0",
		"npm": "^3.0.0"
	},
	"scripts": {
		"clean": "rimraf dist",
		"compile": "better-npm-run compile",
		"lint": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"start": "better-npm-run start",
		"dev": "better-npm-run dev",
		"dev:nw": "npm run dev -- --nw",
		"dev:no-debug": "npm run dev -- --no_debug",
		"test": "better-npm-run test",
		"test:dev": "npm run test -- --watch",
		"deploy": "better-npm-run deploy",
		"flow:check": "babel-node client-bin/flow-check",
		"codecov": "cat coverage/*/lcov.info | codecov"
	},
	"betterScripts": {
		"compile": {
			"command": "node ./node_modules/babel-cli/bin/babel-node.js client-bin/compile",
			"env": {
				"DEBUG": "app:*"
			}
		},
		"dev": {
			"command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js client-bin/server",
			"env": {
				"NODE_ENV": "development",
				"DEBUG": "app:*"
			}
		},
		"deploy": {
			"command": "./deploy.sh",
			"env": {
				"NODE_ENV": "production",
				"DEBUG": "app:*"
			}
		},
		"start": {
			"command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js client-bin/server",
			"env": {
				"DEBUG": "app:*"
			}
		},
		"test": {
			"command": "node ./node_modules/babel-cli/bin/babel-node.js ./node_modules/karma/bin/karma start build/karma.conf",
			"env": {
				"NODE_ENV": "test",
				"DEBUG": "app:*"
			}
		},
		"test-monitor": {
			"command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js ./node_modules/karma/bin/karma start build/karma.conf",
			"env": {
				"NODE_ENV": "test",
				"DEBUG": "app:*"
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pdaddyo/soundbounce-v2.git"
	},
	"author": "Paul Barrass",
	"license": "MIT",
	"dependencies": {
		"autoprefixer-loader": "^3.2.0",
		"babel-polyfill": "^6.16.0",
		"better-npm-run": "0.0.5",
		"co-request": "^1.0.0",
		"compression": "^1.6.2",
		"cookie-parser": "^1.4.3",
		"cookies-js": "^1.2.2",
